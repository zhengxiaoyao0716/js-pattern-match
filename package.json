    "pattern",
    "match"
  ],
  "author": "github.com/zhengxiaoyao0716",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhengxiaoyao0716/js-pattern-match/issues"
  },
  "homepage": "https://github.com/zhengxiaoyao0716/js-pattern-match#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.2.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.1.1",
    "http-server": "^0.10.0"
  }
}
